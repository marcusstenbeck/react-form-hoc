{
  "name": "react-form-hoc",
  "version": "0.0.9",
  "description": "React form HOC",
  "license": "MIT",
  "repository": "stoeffel/react-form-hoc",
  "author": {
    "name": "Christoph Hermann",
    "email": "schtoeffel@gmail.com",
    "url": "stoeffel.github.io"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "build": "babel module --out-dir .",
    "develop":
      "nodangel --ignore node_modules --watch test --watch module --exec 'npm run test'",
    "test": "standard && nyc --require babel-core/register mocha",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "release:major":
      "npm run build && xyz --repo git@github.com:stoeffel/react-form-hoc.git --increment major",
    "release:minor":
      "npm run build && xyz --repo git@github.com:stoeffel/react-form-hoc.git --increment minor",
    "release:patch":
      "npm run build && xyz --repo git@github.com:stoeffel/react-form-hoc.git --increment patch"
  },
  "files": ["index.js"],
  "keywords": ["react", "form", "hoc"],
  "standard": {
    "parser": "babel-eslint",
    "globals": ["describe", "it", "before"]
  },
  "dependencies": {
    "shallowequal": "^0.2.2"
  },
  "devDependencies": {
    "babel": "6.5.1",
    "babel-cli": "6.5.1",
    "babel-core": "6.5.1",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "codecov": "1.0.1",
    "enzyme": "^2.2.0",
    "expect": "^1.18.0",
    "expect-jsx": "^2.5.1",
    "jsdom-test-browser": "4.0.2",
    "mocha": "2.3.4",
    "mocha-lcov-reporter": "1.0.0",
    "nodangel": "1.3.8",
    "nyc": "^6.4.0",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "sinon": "^1.17.3",
    "standard": "^6.0.8",
    "standard-format": "^2.1.1",
    "xyz": "^0.5.0"
  }
}
